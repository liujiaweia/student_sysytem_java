<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="student_system.dao.StudentTagDAO">



    <insert id="save" parameterType="StudentTag" useGeneratedKeys="true" keyProperty="id">
        insert into t_student_tag values(#{id},#{studentid},#{tagid})
    </insert>


</mapper>